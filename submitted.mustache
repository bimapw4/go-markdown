                    <!--                                        <a href="https://privy.id/terms-and-conditions" style="font-size:12px;color:#999;" target="_blank">Ketentuan Penggunaan</a>-->
                    <!--                                    </td>-->
                    <!--                                    <td align="center">-->
                    <!--                                        <a href="https://privy.id/kebijakan-privasi" style="font-size:12px;color:#999;" target="_blank">Kebijakan Privasi</a>-->
                    <!--                                    </td>-->
                    <!--                                    <td align="center">-->
                    <!--                                        <a href="https://repository.privyca.id/" style="font-size:12px;color:#999;" target="_blank">Repository Dokumen</a>-->
                    <!--                                    </td>-->
                    <!--                                </tr>-->
                    <!--                                </tbody>-->
                    <!--                            </table>-->
                    <!--                        </td>-->
                    <!--                    </tr>-->
                    </tbody>
                </table>
            </td>
        </tr>
        </tbody>
    </table>
</div>
